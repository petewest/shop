<% provide(:title, "#{@product.name} stock level") %>

<%= content_tag(:h1, ) do %>
<%= link_to @product.name, @product %> stock levels
<% end %>

<%= render 'product_stock_level', product: @product %>

<%= link_to "Add stock", new_product_stock_level_path(@product), class: "btn btn-default", data: { modal_target: '#modal' } %>
