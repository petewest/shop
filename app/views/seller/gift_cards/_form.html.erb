<%= form_for [:seller, gift_card] do |f| %>
	<div class="form-group">
		<%= f.label :redeemer_id, class: "control-label" %>
		<%= f.collection_select :redeemer_id, User.all, :id, :name, {}, class: "form-control" %>
	</div>
	<%= render 'shared/cost_fields', f: f %>
	<%= f.submit class: "btn btn-default" %>
<% end %>
